# FILE: spex-rust/prompts/Makefile
# Default target for spex-core's build pass
build:
	@echo "--- Building Rust Project ---"
	@cargo build --release

# A target for running tests
test:
	@echo "--- Testing Rust Project ---"
	@cargo test --release

# A target for running the application
run:
	@echo "--- Running Rust Project ---"
	@cargo run --release
